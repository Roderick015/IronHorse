                </div>
                <div class="col-4">
                    <div class="form-group">
                        <label asp-for="InduccionShouxinVigencia" class="control-label"></label>
                    </div>
                </div>
                <div class="col-4">
                    <div class="form-group">
                        <input asp-for="InduccionShouxinVigencia" class="form-control form-control-sm datepicker-input" />
                        <span asp-validation-for="InduccionShouxinVigencia" class="text-danger"></span>
                    </div>
                </div>
            </div>

            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="InduccionTisur" /> @Html.DisplayNameFor(model => model.InduccionTisur)
                </label>
            </div>
            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="InduccionRansa" /> @Html.DisplayNameFor(model => model.InduccionRansa)
                </label>
            </div>
            <div class="form-group form-check">
                <label class="form-check-label">
                    <input class="form-check-input" asp-for="InduccionAcerosA" /> @Html.DisplayNameFor(model => model.InduccionAcerosA)
                </label>
            </div>
            <div class="form-group">
                <input type="submit" value="Grabar" class="btn btn-primary" />
            </div>





        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Regreso</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
    <link href="~/lib/bootstrap-datepicker/css/bootstrap-datepicker.min.css" rel="stylesheet" />
    <script src="~/lib/bootstrap-datepicker/js/bootstrap-datepicker.min.js"></script>
    <script src="~/lib/bootstrap-datepicker/locales/bootstrap-datepicker.es.min.js"></script>

    <script>
    $('.datepicker-input').datepicker({
        language: "es",
        autoclose: true,
        todayHighlight: true
    });
    </script>

}
