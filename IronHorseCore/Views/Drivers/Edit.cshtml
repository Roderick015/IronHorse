                    </div>
                </div>
                <div class="col-4">
                    <div class="form-group">
                        <label asp-for="CursosPortuariosVigencia" class="control-label"></label>
                    </div>
                </div>
                <div class="col-4">
                    <div class="form-group">
                        <input asp-for="CursosPortuariosVigencia" class="form-control form-control-sm datepicker-input" />
                        <span asp-validation-for="CursosPortuariosVigencia" class="text-danger"></span>
                    </div>
                </div>
            </div>
            <div class="form-row">
                <div class="col-4">
                    <div class="form-group form-check">
                        <label class="form-check-label">
                            <input class="form-check-input" asp-for="InduccionImpala" /> @Html.DisplayNameFor(model => model.InduccionImpala)
                        </label>
                    </div>
                </div>
                <div class="col-4">
                    <div class="form-group">
                        <label asp-for="InduccionImpalaVigencia" class="control-label"></label>
                    </div>
                </div>
                <div class="col-4">
                    <div class="form-group">
                        <input asp-for="InduccionImpalaVigencia" class="form-control form-control-sm datepicker-input" />
                        <span asp-validation-for="InduccionImpalaVigencia" class="text-danger"></span>
